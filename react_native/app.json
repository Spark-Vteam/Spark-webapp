{
  "expo": {
    "name": "react_native_app",
    "slug": "react_native_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/IconColorsMobile.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/SparkMobile.png",
      "resizeMode": "contain",
      "backgroundColor": "#00CDA3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SparkMobile.png",
        "backgroundColor": "#00CDA3"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
